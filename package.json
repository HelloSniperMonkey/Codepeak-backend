{
	"name": "ts-backend-template",
	"version": "1.0.0",
	"main": "dist/src/index.js",
	"scripts": {
		"build": "yarn rimraf dist && yarn tsc",
		"prestart": "yarn build",
		"start": "concurrently \"node dist/src/index.js\" \"tsx src/temp\"",
		"dev" : "yarn build; yarn start",
		"serve": "tsc; nodemon dist/src/index.js",
		"format": "prettier --write .",
		"lint": "prettier --write . && eslint ."
	},
	"license": "MIT",
	"dependencies": {
		"@octokit/rest": "^21.1.0",
		"argon2": "^0.41.1",
		"body-parser": "^1.20.3",
		"cookie-parser": "^1.4.7",
		"express": "^4.21.2",
		"express-validator": "^7.2.1",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^6.12.0",
		"mongoose": "^8.9.5",
		"node-fetch": "^3.3.2",
		"rimraf": "^6.0.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.19.0",
		"@types/body-parser": "^1.19.5",
		"@types/cookie-parser": "^1.4.8",
		"@types/express": "^5.0.0",
		"@types/express-validator": "^3.0.2",
		"@types/jsonwebtoken": "^9.0.8",
		"@types/mongodb": "^4.0.7",
		"@types/mongoose": "^5.11.97",
		"@types/node": "^22.10.10",
		"@typescript-eslint/eslint-plugin": "^8.21.0",
		"@typescript-eslint/parser": "^8.21.0",
		"concurrently": "^9.1.2",
		"dotenv": "^16.4.7",
		"eslint": "^9.19.0",
		"eslint-plugin-eslint-plugin": "^6.4.0",
		"globals": "^15.14.0",
		"nodemon": "^3.1.9",
		"prettier": "^3.4.2",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0"
	},
	"packageManager": "yarn@4.6.0"
}
